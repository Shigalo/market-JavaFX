    <Button fx:id="profile" layoutX="468.0" mnemonicParsing="false" text="Профиль" />
    <TableView fx:id="table" layoutX="57.0" layoutY="142.0" prefHeight="200.0" prefWidth="497.0">
        <columns>
            <TableColumn fx:id="seller" prefWidth="120.0" text="Продавец" />
            <TableColumn prefWidth="282.0" text="Количество сделок">
                <columns>
