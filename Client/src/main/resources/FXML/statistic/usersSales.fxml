            <TableColumn fx:id="cost" minWidth="0.0" prefWidth="81.0" text="Суммарная&#10;  выручка" />
        </columns>
    </TableView>
    <Button fx:id="chart" layoutX="264.0" layoutY="61.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="Диаграмма" />
</AnchorPane>
