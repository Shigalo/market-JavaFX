<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="filonenko.sales.controllers.Users"
            prefHeight="400.0" prefWidth="600.0">
    <MenuBar>
        <Menu mnemonicParsing="false" text="Данные">
            <MenuItem fx:id="usersMenu" mnemonicParsing="false" text="Пользователи" />
            <MenuItem fx:id="hardwareMenu" mnemonicParsing="false" text="Техника" />
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Delete" />
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About" />
        </Menu>
    </MenuBar>
    <TableView fx:id="table" layoutX="125.0" layoutY="100.0" prefHeight="200.0" prefWidth="227.0">
        <columns>
            <TableColumn fx:id="login" prefWidth="75.0" text="Логин" />
            <TableColumn fx:id="name" prefWidth="75.0" text="Имя" />
            <TableColumn fx:id="access" prefWidth="75.0" text="Доступ" />
        </columns>
    </TableView>
    <Button fx:id="log" layoutX="550.0" mnemonicParsing="false"/>
</AnchorPane>
