<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="filonenko.sales.controllers.Registration">
    <MenuBar>
        <Menu mnemonicParsing="false" text="Данные">
            <MenuItem fx:id="usersMenu" mnemonicParsing="false" text="Пользователи" />
            <MenuItem fx:id="productMenu" mnemonicParsing="false" text="Продукция" />
            <MenuItem fx:id="salesMenu" mnemonicParsing="false" text="Продажи" />
            <MenuItem fx:id="storageMenu" mnemonicParsing="false" text="Склад" />
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Delete" />
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About" />
        </Menu>
    </MenuBar>
    <Button fx:id="log" layoutX="550.0" mnemonicParsing="false" />
    <GridPane layoutX="101.0" layoutY="92.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Label prefHeight="45.0" prefWidth="141.0" text="Введите имя" />
            <Label prefHeight="45.0" prefWidth="141.0" text="Введите логин" GridPane.rowIndex="1" />
            <Label prefHeight="35.0" prefWidth="111.0" text="Введите пароль" GridPane.rowIndex="2" />
            <Label prefHeight="35.0" prefWidth="111.0" text="Повторите пароль" GridPane.rowIndex="3" />
            <TextField fx:id="name" GridPane.columnIndex="1" />
            <TextField fx:id="login" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <PasswordField fx:id="passwordConfirm" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        </children>
    </GridPane>
   <Button fx:id="registration" layoutX="152.0" layoutY="234.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="148.0" text="Зарегистрироваться" />
   <Button fx:id="cancel" layoutX="182.0" layoutY="278.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Отмена" />
</AnchorPane>
